<?xml version="1.0" encoding="UTF-8"?>

<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
        xmlns:android="http://schemas.android.com/apk/res/android"
        id="cordova-plugin-tdScanPicture"
        version="0.0.1">
    
    <name>TDScanPicture</name>
    
    <description>This plugin for Cordova</description>
    
    <keywords>Cordova</keywords>
    
    <repo>https://github.com/terrydr/com.terrydr.exit.git</repo>
    <issue>https://github.com/terrydr/com.terrydr.exit.git</issue>
    
    <license>MIT</license>
    
    <engines>
        <engine name="cordova" version=">=3.0.0" />
    </engines>
    
    <js-module src="www/TDScanPicture.js" name="TDScanPicture">
        <clobbers target="tdScanPicture" />
    </js-module>
    
    <platform name="ios">
        <config-file  target="config.xml" parent="/*">
            <feature  name="TDScanPicture">
                <param  name="ios-package"  value="TDScanPicture"/>
            </feature>
        </config-file>
        
        <source-file src="src/ios/Class/TDScanPicture.m" />
        <header-file src="src/ios/Class/TDScanPicture.h" />
        <header-file src="src/ios/Class/TDHeader.h" />
        <source-file src="src/ios/Class/TDNavgationController.m" />
        <header-file src="src/ios/Class/TDNavgationController.h" />
        <source-file src="src/ios/Class/UIImage+MLTint.m" />
        <header-file src="src/ios/Class/UIImage+MLTint.h" />
        <source-file src="src/ios/Class/UIView+MLExtension.m" />
        <header-file src="src/ios/Class/UIView+MLExtension.h" />
        <source-file src="src/ios/Class/UIView+AutoLayoutViews.m" />
        <header-file src="src/ios/Class/UIView+AutoLayoutViews.h" />
        <source-file src="src/ios/Class/UIView+Extension.m" />
        <header-file src="src/ios/Class/UIView+Extension.h" />
        <source-file src="src/ios/Class/LX_layoutAttributes.m" />
        <header-file src="src/ios/Class/LX_layoutAttributes.h" />
        <header-file src="src/ios/Class/MLSelectPhotoCommon.h" />
        <source-file src="src/ios/Class/MLSelectPhotoBrowserViewController.m" />
        <header-file src="src/ios/Class/MLSelectPhotoBrowserViewController.h" />
        <source-file src="src/ios/Class/MLSelectPhotoPickerBrowserPhotoScrollView.m" />
        <header-file src="src/ios/Class/MLSelectPhotoPickerBrowserPhotoScrollView.h" />
        <source-file src="src/ios/Class/MLSelectPhotoPickerBrowserPhotoView.m" />
        <header-file src="src/ios/Class/MLSelectPhotoPickerBrowserPhotoView.h" />
        <source-file src="src/ios/Class/MLSelectPhotoPickerBrowserPhotoImageView.m" />
        <header-file src="src/ios/Class/MLSelectPhotoPickerBrowserPhotoImageView.h" />
    </platform>
    
</plugin>
